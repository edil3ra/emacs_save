odoubleMe x =
  x + x
doubleUs x y =
  x * 2  + y * 2


doubleSmallNumber x = if x > 100
                      then x
                      else x*2
  
doubleSmallNumber' x = (if x > 100 then x else x * 2) + 1

it'sme = "Conan O'Brien"

boomBangs xs =
  [if x < 10 then "boom" else "bangs" | x <- xs, odd x]
  
a = [(a, b, c) | c <- [1..10],  a <- [1..c], b <- [1..a],
     sum([a, b, c]) == 24, a^2 + b^2 == c^2 ]





