import random

l = [1, 3, 4, 5, 6, 7, 9, 11, 14, 16]


def find_element_seq(l, el):
    for index, value in enumerate(l):
        if value == el:
            return  index
    else:
        return




def binary_search(l, el, low=0, high=len(l)):
    mid = (low + high) // 2
    
    if low > high:
        return False

    if el == l[mid]:
        return True
    elif el < l[mid]:
        return  binary_search(l, el, low, mid-1)
    else:
        return binary_search(l, el, mid + 1, high)


d = binary_search(l, 13)
print(d)
